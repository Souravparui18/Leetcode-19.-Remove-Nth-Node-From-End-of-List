class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        ListNode dummy = new ListNode(0);

        dummy.next = head;


        ListNode l = head;

        int jump = 0;

        while(jump < n && l != null) {
            l = l.next;
            jump++;

        }

        ListNode prev = dummy,
                 curr = l;


        while(curr != null) {
            curr = curr.next;
            prev = prev.next;
            }

       prev.next = prev.next.next;
        return dummy.next;
        
    }
}
